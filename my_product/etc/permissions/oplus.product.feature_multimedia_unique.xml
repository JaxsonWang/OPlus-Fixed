<oplus-config>
    <!-- audio start -->
    <!-- Tansen@MULTIMEDIA.AUDIOSERVER.FRAMEWORK, 2022/06/21, add for alert slider -->
    <oplus-feature name="oplus.software.audio.alert_slider" />
    <!-- Tansen@MULTIMEDIA.AUDIOSERVER.FRAMEWORK, 2022/06/21, add for haptic -->
    <oplus-feature name="oplus.software.audio.haptic_channel_support"/>
    <oplus-feature name="oplus.software.haptic_vibrator_v1.support"/>
    <!-- Zehua.Zhang@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2023/05/15. Add for media control -->
    <oplus-feature name="oplus.software.audio.media_control"/>
    <!-- Rui.Zhang@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2021/7/29. Add for hearing health feature -->
    <oplus-feature name="oplus.software.audio.hearing_health_support"/>
    <!-- Rui.Zhang@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2021/7/29. Add for spl limit -->
    <oplus-feature name="oplus.software.audio.spl_limit_support"/>
    <!-- Lvhongzhou@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2023/8/17. Add for speaker channel reserve -->
    <oplus-feature name="oplus.software.audio.dualspk_support"/>
    <!-- zhanglixia@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2023/05/19. Add for spk oplus spatializer -->
    <oplus-feature name="oplus.software.spatializer_speaker"/>
    <!-- zhanglixia, 2023/05/30, Add for bluetooth volume sync -->
    <oplus-feature name="oplus.software.bluetooth_volume_sync"/>
    <!-- WangDong4@MULTIMEDIA.AUDIOSERVER.BASE, 2024/02/26, add for multi-application volume adjustment -->
    <oplus-feature name="oplus.software.multi_app.volume.adjust.support"/>
    <!-- YuanBin@MULTIMEDIA.AUDIOSERVER.BASE, 2024/04/16, Add for voice assistant independent volume -->
    <oplus-feature name="oplus.software.audio.assistant_volume_support"/>
    <!-- Zehua.Zhang@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2023/05/15. Add for volume default adjust -->
    <oplus-feature name="oplus.software.audio.volume_default_adjust"/>
    <!-- Liang.Xu@MULTIMEDIA.AUDIOSERVER.FRAMEWORK, 2023/12/07, Add for Oplus Spatializer Record. -->
    <oplus-feature name="oplus.software.audio_spatializer_record" />
    <!-- YuanBin@MULTIMEDIA.AUDIOSERVER.BASE, 2024/04/16, Add for voice assistant independent volume -->
    <oplus-feature name="oplus.software.audio.assistant_volume_support"/>
    <!-- Mengdie.Zhou@MULTIMEDIA.AUDIOSERVER.EFFECT.2024/07/13. Add for audio effect combined -->
    <oplus-feature name="oplus.software.audio.effect.combined"/>
    <!-- Xin.Zhao@MULTIMEDIA.AUDIOSERVER.FRAMEWORK, 2025/02/26, Add for Oplus Music Source Separation. -->
    <!--<oplus-feature name="oplus.software.audio.mss_music_only" />-->
    <!-- audio end -->

    <!-- AUDIODRIVER start -->
    <!-- YeWenliang@MULTIMEDIA.AUDIODRIVER, 2021/08/26, Add for lvacfs surround record -->
    <oplus-feature name="oplus.software.video.surround_record_support"/>
    <!-- AUDIODRIVER end -->

    <!-- WangDong4@MULTIMEDIA.AUDIOSERVER.FRAMEWORK, 2024/05/09, Add for vocal prominence 2.0 -->
    <oplus-feature name="oplus.hardware.audio.voice_isolation_support_v2" />

    <!-- video start -->
    <oplus-feature name="oplus.software.video.sr_support"/>
    <oplus-feature name="oplus.software.video.osie_support"/>
    <!-- video end -->
    <!-- sceen mode feature -->
    <oplus-feature name="oplus.software.display.dynamic_fps_switch" />
    <!-- performance feature -->
    <oplus-feature name="oplus.software.display.refreshrate_default_smart"/>
    <oplus-feature name="oplus.software.display.multi_device_decrease_refresh_rate"/>
    <oplus-feature name="oplus.software.display.adfr"/>
    <!-- Game yuanshen 4D vibration feature -->
    <oplus-feature name="oplus.software.game.yuanshenvibration"/>
</oplus-config>
